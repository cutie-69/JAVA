class Box
{
    double width;
    double height;
    double depth;

    // Constructor to initialize dimensions of the box
    Box(double w, double h, double d)
    {
        width = w;
        height = h;
        depth = d;
    }

    // Method to calculate and return the volume of the box
    double volume()
    {
        return width * height * depth;
    }
}

// Subclass BoxWeight inherits from Box
class BoxWeight extends Box
{
    double weight; // Additional field specific to BoxWeight

    // Constructor to initialize dimensions and weight
    BoxWeight(double w, double h, double d, double m)
    {
        super(w, h, d); // Call superclass constructor to initialize dimensions
        weight = m;
    }
}

public class Demoinheritance
{
    public static void main(String[] args)
    {
        // Create an object of BoxWeight and initialize all fields
        BoxWeight mybox = new BoxWeight(10, 5, 2, 200);
        System.out.println("The volume of child Box is " + mybox.volume());
        System.out.println("The weight of child Box is " + mybox.weight);
    }
}
